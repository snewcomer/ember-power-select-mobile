<h1>SCOTT</h1>

<!-- single / nav component provided -->
{{#power-select-mobile 
  renderInPlace=true 
  navMobileComponent="nav-mobile" 
  options=numbers 
  mustFallback=mustFallback 
  selected=number 
  onchange=(action (mut number)) as |num|}}
  {{num}}
{{/power-select-mobile}}

<!-- multiple / no nav component provided -->
{{#power-select-mobile 
  multiple=true 
  renderInPlace=true 
  options=numbers 
  mustFallback=mustFallback 
  selected=number 
  onchange=(action (mut number)) as |num|}}
  {{num}}
{{/power-select-mobile}}
